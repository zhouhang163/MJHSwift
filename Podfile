platform :ios, '8.0'
inhibit_all_warnings!
use_frameworks!
target 'MJHSwift' do
    #网络请求
    pod 'AFNetworking'
    #自动布局
    pod 'Masonry'
    #图片
    pod 'SDWebImage'
    #loading指示器
    pod 'SVProgressHUD'
    #空数据展示
    pod 'DZNEmptyDataSet'
    #键盘事件
    pod 'IQKeyboardManager'
    #刷新加载
    pod 'MJRefresh'
    #高德地图
    pod 'AMap3DMap'
    pod 'AMapSearch'
    pod 'AMapLocation'
end  
